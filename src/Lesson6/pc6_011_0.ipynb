    "# 구구단 클래스 정의\n",
    "\n",
    "class 구구단():\n",
    "    def print(self, num):\n",
    "        for i in range(1, 10):\n",
    "            print(\"{}x{}={}\".format(num, i, num * i))\n",
    "\n",
    "m = 구구단()\n",
    "m.print(3)"
